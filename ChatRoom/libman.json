{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "font-awesome@5.15.4",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/*"
      ]
    },
    {
      "library": "microsoft-signalr@8.0.0",
      "destination": "wwwroot/lib/microsoft-signalr/",
      "files": [
        "signalr.min.js"
      ]
    },
    {
      "library": "dompurify@3.1.3",
      "destination": "wwwroot/lib/dompurify/",
      "files": [
        "purify.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "popper.js@2.11.8",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "umd/popper.min.js"
      ]
    }
  ]
}